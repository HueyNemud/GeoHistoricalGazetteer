@prefix ghp:  <http://data.geohistoricaldata.org/def/places#> .
@prefix dul:  <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix time:  <http://www.w3.org/2006/time#> .
@prefix locn:  <http://www.w3.org/ns/locn#> .
@prefix dc:  <http://purl.org/dc/elements/1.1/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix gsp:  <http://www.opengis.net/ont/geosparql#> .
@prefix lp:  <http://linkedplaces.org/places#> .

# A sample place.
# - named Graffoudet in 1756 (source: Cassini)
#         Griffandeix in 1791 (source: assembl√©e nationale consistuante) 
# - located near Clermont-Ferrand Point(7.44096242532983 48.5997553804943), CRS:WGS84
<http://data.geohistoricaldata.org/id/place/t_28936>
  a                        ghp:Place ; # subclassOf : dul:PhysicalPlace, lp:Place
  ghp:hasName              <http://data.geohistoricaldata.org/id/placename/t_28936_1> ;
  ghp:hasName              <http://data.geohistoricaldata.org/id/placename/t_28936_2> ;
  ghp:hasLocation          <http://data.geohistoricaldata.org/id/location/t_28936> ;

  # Not in the GHD ontology
  dc:creator               "The GeoHistoricalData group. http://geohistoricaldata.org" ;
  dc:identifier            "t_28936" .

<http://data.geohistoricaldata.org/id/placename/t_28936_1>
  a                         ghp:Name ; # subclassOf : dul:Quality, lp:Name
  dul:hasDataValue          "Griffoudet"^^xsd:String;
  ghp:attestedIn             <http://data.geohistoricaldata.org/id/source/cassini>.

<http://data.geohistoricaldata.org/id/placename/t_28936_2>
  a                         ghp:Name ; # subclassOf : dul:Quality, lp:Name
  dul:hasDataValue          "Griffandeix"^^xsd:String;
  ghp:attestedIn             <http://data.geohistoricaldata.org/id/source/decretsassembleeconstituante>;
  # This time we want to set a valid time more specific than in the source
  dul:isObservableAt        [
                                a    time:Instant; # subclassOf dul:TimeInterval, time:TemporalEntity
                                time:inXSDDate  "1791-07-12"
                            ].

<http://data.geohistoricaldata.org/id/location/t_28936>
  a                         ghp:Location ; # subclassOf : lp:Location, dul:Quality
  dul:hasRegion             [ # SpaceRegions are stored as blanknodes. Here, the locn:geometry is a wktLiteral
                                a    dul:SpaceRegion;
                                ghp:geometry "<http://www.opengis.net/def/crs/OGC/1.3/CRS84> POINT(7.44096242532983 48.5997553804943)"^^gsp:wktLiteral
                            ];
  ghp:attestedIn             <http://data.geohistoricaldata.org/id/source/cassini>.

<http://data.geohistoricaldata.org/id/source/cassini>
  a dul:InformationRealization;
  dc:identifier "info:doi/some.fancy.doi";
  ghp:isObservableAt [
      a time:Interval;
      time:hasBeginning [
        a time:Instant;
        time:inXSDgYear "1756"^^xsd:gYear 
      ];
      time:hasEnd [
        a time:Instant;
        time:inXSDgYear "1799"^^xsd:gYear 
      ]
    ].

<http://data.geohistoricaldata.org/id/source/decretsassembleeconstituante>
a dul:InformationObject;
  dc:identifier "info:doi/some.other.fancy.doi";
  ghp:isObservableAt [
      a time:Interval;
      time:hasBeginning [
        a time:Instant;
        time:inXSDgYear "1791"^^xsd:gYear 
      ];
      time:hasEnd [
        a time:Instant;
        time:inXSDgYear "1793"^^xsd:gYear 
      ]
    ].