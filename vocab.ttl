time:TemporalEntity
  rdf:type owl:Class ;
  rdfs:comment "A temporal interval or instant."@en ;
  rdfs:label "Temporal entity"@en ;
  rdfs:subClassOf owl:Thing ;
  owl:unionOf (
      time:Instant
      time:Interval
    ) ;
  skos:definition "A temporal interval or instant."@en.

