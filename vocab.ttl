# Nouvelles classes

:Place a owl:Class ;
	rdfs:subClassOf dul:PhysicalPlace, pleiades:Place.

:Location a owl:Class ;
	rdfs:subClassOf dul:Quality, pleiades:Location.

:Name a owl:Class ;
	rdfs:subClassOf dul:Quality, pleiades:Name .

# Statut d'un lieu (e.g chef lieu de commune)
:Status a owl:Class;
	rdfs:subClassOf dul:Quality ;
	rdfs:label "Status"@fr . 

:Population a owl:Class;
	rdfs:subClassOf dul:Quality ;
	rdfs:label "Population"@fr .


# ------------------------
# Nouvelles propriétés

#-- Temporalisation 
# BERTRAND : On redéfinit isObservableAt de façon à avoir un range plus large que dul:TimeInterval. À lier avec dul:isObservableAt ?
:isObservableAt	a	owl:ObjectProperty ;
  rdfs:domain dul:Entity ;
  rdfs:range 
    [
      a owl:Class ;
			owl:unionOf (dul:TimeInterval time:TemporalEntity skos:Concept)
    ]	.

#-- Source
# BERTRAND : On redéfinit cito:citesAsEvidence de façon à avoir dul:InformationEntity (soit InformationObject, soit InformationRealization) comme range
:attestedIn a owl:ObjectProperty ;
	rdfs:subPropertyOf prov:wasDerivedFrom, dul:associatedWith ;
	rdfs:range dul:InformationEntity .

#-- Liens entre un lieu et ses qualités.
:hasName  a owl:ObjectProperty ;
	rdfs:subPropertyOf dul:hasQuality, pleiades:hasName ;
	rdfs:domain :Place ;
 	rdfs:range :Name .

:hasLocation  a owl:ObjectProperty ;
	rdfs:subPropertyOf dul:hasQuality, pleiades:hasLocation ;
	rdfs:domain :Place ;
 	rdfs:range :Location .

:hasStatus a owl:ObjectProperty ;
	rdfs:subPropertyOf dul:hasQuality ;
	rdfs:domain :Place ;
 	rdfs:range :Status .

:hasPopulation a owl:ObjectProperty ;
	rdfs:subPropertyOf dul:hasQuality ;
	rdfs:domain :Place ;
 	rdfs:range :Population .

:hasInseeCode a owl:ObjectProperty ;
	rdfs:subPropertyOf dul:hasQuality ;
	rdfs:domain :Place ;
 	rdfs:range dul:Quality .

:geometry a owl:ObjectProperty ;
	rdfs:subPropertyOf dul:hasRegionDataValue, locn:geometry ;
	rdfs:domain dul:SpaceRegion ;
	rdfs:range locn:Geometry .

